# 반복적으로 구현한 factorial
def factorial_iterative(n):
    result = 1
    # 1에서 n까지의 수를 곱하기
    for i in range(1, n + 1):
        result *= i
    return result
    
# 재귀적으로 구현한 factorial
def factorial_recursive(n)
    if n <= 1: # n이 1 이하인 경우 1 반환
        return 1
    # n! 을 그대로 코드로 작성.
    return n * factorial_recursive(n - 1)
